#include <ecom/registryinfov2.rh>
#include "IptvEngineUids.h"


// ---------------------------------------------------------------------------
// theInfo
// ECOM plugin information
// ---------------------------------------------------------------------------
//
RESOURCE REGISTRY_INFO theInfo
	{
    // use resource format version 2 to enable ROM only plugins
	resource_format_version = RESOURCE_FORMAT_VERSION_2;
	dll_uid = KIptvScheduledDownloadPluginDllUid;
	interfaces = 
		{
		INTERFACE_INFO
            {
	        interface_uid = KIptvScheduledDownloadPluginInterfaceUid;
	        implementations = 
                {
	            IMPLEMENTATION_INFO
                    {
                    implementation_uid = KIptvScheduledDownloadPluginImplementationUid;
                    version_no = 1;
                    display_name = "";
                    default_data = "";
                    opaque_data = "";
                    rom_only = 0; // This is not a ROM only plugin
    			    }
    	        };
            }
		};	
	}
