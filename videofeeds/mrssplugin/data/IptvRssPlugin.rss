#include <ecom/registryinfo.rh>
#include "IptvEngineUids.h"

// -----------------------------------------------------------------------------
// registry_info
//
// -----------------------------------------------------------------------------
//
RESOURCE REGISTRY_INFO registry_info
    {
    // UID for the DLL
    dll_uid = KIptvRssPluginDllUid;

    // Declare array of interface info
    interfaces =
        {
        INTERFACE_INFO
            {
            // UID of interface that is implemented
            interface_uid = KIptvEcgPluginInterfaceUid;
            implementations = 
                {
                IMPLEMENTATION_INFO
                    {
                    implementation_uid  =  KIptvRssPluginImplementationUid;
                    version_no          =  1;
                    display_name        =  "";
                    default_data        =  "";
                    opaque_data         =  "";
                    }
                };
            }
        };
    }
